<section id="portfolio">
    <div class="container">
        <h1>{{cocktailName}}</h1>
        <div class="row">
            <div class="col-md-8 col-xs-12">
                <div class="col-md-12 col-xs-12">
                    <img class="cocktail-image" src="{{imgUrl}}">
                </div>

                <div class="col-md-6 col-xs-12">
                    <h3>Ingredients</h3>
                    <ul class="list-group">
                        {{#each ingredients}}
                            <li class="list-group-item">{{this}}</li>
                        {{/each}}
                    </ul>
                </div>
                <div class="col-md-6 col-xs-12">
                    <h3>Directions</h3>
                    {{instructions}}
                </div>
            </div>

            <div class="col-md-4 col-xs-12">
                This is where right side stuff will go.
            </div>
        </div>
    </div>
</section>


